{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes": "off",
        "linebreak-style": "off",
        "no-console": "off",
        "comma-dangle": "off",
        "implicit-arrow-linebreak": "off",
        "object-curly-newline": "off",
        "no-else-return": "off",
        "no-lonely-if": "off",
        "operator-linebreak": "off",
        "prefer-template": "off",
        "function-paren-newline": "off",
        "indent": "off"
    }
}
